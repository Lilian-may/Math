/* Произведение последовательности квадратов чисел. */

package main

import "fmt"

func main() {
    pr := 1.0
    var i, b float64
    fmt.Println("Введите число начала последовательности: ")
    fmt.Scan(&i)
    fmt.Println("Введите конечное число последовательности: ")
    fmt.Scan(&b)
    i = 1.0
    for ; i <= b; i++ {
        pr *= i * i
}
if pr < 9999999.9{
fmt.Println("Результат: \n", pr)
} else if pr > 9999999.9 {
    fmt.Printf("%e", pr)
}
}
