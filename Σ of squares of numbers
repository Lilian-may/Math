/* Находит Σ квадратов чисел */

package main

import "fmt"

func main() {
    sum := 0.0
    var i, b float64
    fmt.Println("Введите число начала последовательности:")
    fmt.Scan(&i)
    fmt.Println("Введите число конца последовательности:")
    fmt.Scan(&b)
    for ; i <= b; i++ {
        sum += i * i
}
if sum < 9999999.9{
fmt.Println(i, "Σ=", sum)
} else if sum > 999999999.9 {
    fmt.Printf("%e", sum)
}
}
